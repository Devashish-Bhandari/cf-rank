{
    "manifest_version":2,
    "name": "CF- Rank",
    "description": "Checks the ranks of your friends at any instance",
    "version": "0.0.1",
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js": ["Home.js"]
        }
    ],
    "browser_action": { 
        "default_title":"CF- Rank",
        "default_icon": "logo-cfr.png",
        "default_popup": "index.html"
    },
    "permissions": ["activeTab"]
}